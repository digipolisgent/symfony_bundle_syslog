{
    "name": "digipolisgent/syslog-bundle",
    "description": "Configures the symfony (monolog) syslog logger to output in a format that kibana can parse.",
    "type": "symfony-bundle",
    "require": {
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/config": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "symfony/http-kernel": "^2.8|^3.0",
        "symfony/monolog-bundle": "^2.8|^3.0",
        "symfony/security": "^2.8|^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jelle Sebreghts",
            "email": "sebreghts.jelle@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "DigipolisGent\\SyslogBundle\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
